  "license": "MIT",
  "scripts": {
    "start": "tsc -p ./server  && concurrently \"tsc -w -p ./server\" \"npm run assets-watch\" \"nodemon dist/server/server.js\"",
    "test": "ng test --watch=false",
    "build": "npm run assets && tsc -p ./server",
    "assets-watch": "cpx \"./server/**/*.{html,mustache,css}\" \"./dist/server\" -w",
    "assets": "cpx \"./server/**/*.{html,mustache,css}\" \"./dist/server\""
  },
  "author": "Ville-Matti Riihikoski",
