{
  "name": "virannomaiset-laji-fi",
  "version": "1.0.0",
  "description": "Private protal for finnish authorities",
  "main": "server/app.js",
  "license": "MIT",
  "scripts": {
    "start": "tsc -p ./server  && concurrently \"tsc -w -p ./server\" \"yarn run assets-watch\" \"nodemon dist/server/bin/www.js\"",
    "test": "ng test --watch=false",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "build": "yarn run assets && tsc -p ./server",
    "assets-watch": "cpx \"./server/**/*.{html,mustache,css}\" \"./dist/server\" -w",
    "assets": "cpx \"./server/**/*.{html,mustache,css}\" \"./dist/server\"",
    "postinstall": "yarn run build"
  },
  "author": "Ville-Matti Riihikoski",
  "repository": {
    "url": "https://bitbucket.org/luomus/viranomaisportaali-backend"
  },
  "private": true,
  "dependencies": {
    "@types/body-parser": "1.16.8",
    "@types/compression": "0.0.36",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "2.8.3",
    "@types/express": "^4.11.1",
    "@types/express-session": "1.15.8",
    "@types/helmet": "0.0.37",
    "@types/jasmine": "^2.8.6",
    "@types/jsonwebtoken": "^7.2.6",
    "@types/node": "^9.4.7",
    "@types/node-uuid": "0.0.28",
    "@types/passport": "^0.4.3",
    "@types/passport-local": "^1.0.32",
    "@types/request": "2.47.0",
    "@types/winston": "^2.3.8",
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
