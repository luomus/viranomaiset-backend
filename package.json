{
  "name": "virannomaiset-laji-fi",
  "version": "1.0.0",
  "description": "Private protal for finnish authorities",
  "main": "server/app.js",
  "license": "MIT",
  "scripts": {
    "start": "tsc -p ./server  && concurrently \"tsc -w -p ./server\" \"npm run assets-watch\" \"nodemon dist/server/bin/www.js\"",
    "test": "ng test --watch=false",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "build": "npm run assets && tsc -p ./server",
    "assets-watch": "cpx \"./server/**/*.{html,mustache,css}\" \"./dist/server\" -w",
    "assets": "cpx \"./server/**/*.{html,mustache,css}\" \"./dist/server\"",
    "postinstall": "npm run build"
  },
  "author": "Ville-Matti Riihikoski",
  "repository": {
    "url": "https://bitbucket.org/luomus/viranomaisportaali-backend"
  },
  "private": true,
  "dependencies": {
    "@types/body-parser": "0.0.32",
    "@types/compression": "0.0.33",
    "@types/cookie-parser": "^1.3.30",
    "@types/cors": "0.0.33",
    "@types/express": "^4.0.32",
    "@types/express-session": "0.0.32",
    "@types/helmet": "0.0.33",
    "@types/jasmine": "^2.2.30",
    "@types/jsonwebtoken": "^7.1.31",
    "@types/node": "^6.0.48",
    "@types/node-uuid": "0.0.28",
    "@types/passport": "^0.3.1",
    "@types/passport-local": "^1.0.29",
    "@types/request": "0.0.38",
    "body-parser": "^1.13.3",
    "compression": "^1.6.2",
    "cookie-parser": "^1.3.5",
    "core-js": "^2.4.1",
    "cors": "^2.8.1",
    "cpx": "^1.5.0",
    "express": "^4.13.4",
    "express-jwt": "^3.3.0",
    "express-session": "^1.14.2",
    "helmet": "^3.3.0",
    "jsonwebtoken": "^5.7.0",
    "mustache-express": "^1.2.4",
    "node-uuid": "^1.4.7",
    "normalize.css": "^5.0.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "request": "^2.79.0",
    "rimraf": "^2.5.4",
