    "cpx": "^1.5.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "express-session": "^1.15.6",
    "helmet": "^3.12.0",
    "jsonwebtoken": "^8.2.0",
    "mustache-express": "^1.2.5",
    "node-uuid": "^1.4.8",
    "normalize.css": "^8.0.0",
